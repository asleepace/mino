{
  "files.exclude": {
    "out": false,
    "node_modules": true,
    ".vscode-test": true
  },
  "search.exclude": {
    "out": true,
    "node_modules": true,
    ".vscode-test": true
  },
  "typescript.tsc.autoDetect": "off",
  "typescript.preferences.includePackageJsonAutoImports": "auto",
  "typescript.suggest.autoImports": true,
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": "keyword.control.directive.mino",
        "settings": {
          "foreground": "#FF1493",
          "fontStyle": "bold"
        }
      },
      {
        "scope": "entity.name.function.css.mino",
        "settings": {
          "foreground": "#9B59B6",
          "fontStyle": "bold"
        }
      },
      {
        "scope": "entity.name.function.html.mino",
        "settings": {
          "foreground": "#E74C3C",
          "fontStyle": "bold"
        }
      },
      {
        "scope": "variable.parameter.mino",
        "settings": {
          "foreground": "#3498DB",
          "fontStyle": "italic"
        }
      },
      {
        "scope": "punctuation.definition.parameters.begin.mino, punctuation.definition.parameters.end.mino",
        "settings": {
          "foreground": "#FF1493"
        }
      },
      {
        "scope": "punctuation.separator.parameter.mino",
        "settings": {
          "foreground": "#7F8C8D"
        }
      },
      {
        "scope": "punctuation.definition.template-expression.begin.mino, punctuation.definition.template-expression.end.mino",
        "settings": {
          "foreground": "#FF6B35",
          "fontStyle": "bold"
        }
      },
      {
        "scope": "punctuation.definition.block.begin.mino, punctuation.definition.block.end.mino",
        "settings": {
          "foreground": "#FF1493",
          "fontStyle": "bold"
        }
      },
      {
        "scope": "entity.name.tag.html",
        "settings": {
          "foreground": "#4A90E2",
          "fontStyle": "bold"
        }
      },
      {
        "scope": "entity.other.attribute-name.html",
        "settings": {
          "foreground": "#F5A623",
          "fontStyle": "italic"
        }
      },
      {
        "scope": "support.type.property-name.css",
        "settings": {
          "foreground": "#7ED321",
          "fontStyle": "normal"
        }
      },
      {
        "scope": "support.constant.property-value.css",
        "settings": {
          "foreground": "#BD10E0"
        }
      },
      {
        "scope": "entity.name.tag.css, entity.other.attribute-name.class.css, entity.other.attribute-name.id.css",
        "settings": {
          "foreground": "#50E3C2",
          "fontStyle": "bold"
        }
      },
      {
        "scope": "constant.other.color.rgb-value.css",
        "settings": {
          "foreground": "#FF6B35"
        }
      },
      {
        "scope": "constant.numeric.css",
        "settings": {
          "foreground": "#4A90E2"
        }
      },
      {
        "scope": "support.function.css",
        "settings": {
          "foreground": "#9B59B6"
        }
      },
      {
        "scope": "meta.html.block.mino",
        "settings": {
          "background": "#2C3E5008"
        }
      },
      {
        "scope": "meta.css.block.mino",
        "settings": {
          "background": "#34495E08"
        }
      },
      {
        "scope": "meta.template.expression.mino",
        "settings": {
          "background": "#FF6B3515"
        }
      },
      {
        "scope": "comment.line.double-slash.mino, comment.block.mino",
        "settings": {
          "foreground": "#7F8C8D",
          "fontStyle": "italic"
        }
      }
    ]
  },
  "files.associations": {
    "*.mino": "mino"
  },
  "[mino]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 2,
    "editor.detectIndentation": false,
    "editor.autoIndent": "full",
    "editor.formatOnSave": false,
    "editor.wordWrap": "on",
    "editor.bracketPairColorization.enabled": true,
    "editor.guides.indentation": true,
    "editor.renderWhitespace": "boundary",
    "editor.semanticHighlighting.enabled": true
  }
}